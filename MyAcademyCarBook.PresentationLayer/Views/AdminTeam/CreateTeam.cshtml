@model MyAcademyCarBook.EntityLayer.Concrete.Team
@{
    ViewData["Title"] = "CreateTeam";
    Layout = "~/Views/_AdminLayout/Index.cshtml";
}

<h1>Takım Arkadaşı eklme Sayfası</h1>

<form method="post">
    <label>Çalışan Adı Soyadı</label>
    <input type="text" asp-for="TeamNameSurname" class="form-control" style="border:1px solid #2222" />
    <br />
    <label>Çalışan Ünvanı</label>
    <input type="text" asp-for="TeamTitle" class="form-control" style="border:1px solid #2222" />
    <br />
    <label>Çalışan Açıklama</label>
    <input type="text" asp-for="Description" class="form-control" style="border:1px solid #2222" />
    <br />
    <label>Çalışan Resim</label>
    <input type="text" asp-for="ImageUrl" class="form-control" style="border:1px solid #2222" />
    <br />
    <button class="btn btn-primary">Referans Kaydet</button>
</form>